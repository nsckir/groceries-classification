    "import glob\n",
    "import os\n",
    "import tarfile\n",
    "list = glob.glob('*/*tar')\n",
    "\n",
    "for f in list:\n",
    "    if f.endswith('tar'):\n",
    "        d = os.path.dirname(f)\n",
    "        print d\n",
    "        tar = tarfile.open(f)\n",
    "        tar.extractall(d)\n",
    "        tar.close()"
   ]
